<h1>Login</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_for(:user, url: sessions_path) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <br/>
    <%= f.text_field :name %>
  </div>
  <p/>
  <div class="field">
    <%= f.label :password %>
    <br/>
    <%= f.password_field :password %>
  </div>
  <br/>
  <div class="actions">
    <%= f.submit "Login" %>
  </div>
<% end %>
